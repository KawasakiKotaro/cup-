<div>
    <% @tweets.each do |tweet| %>
     <p> <%= tweet.text %> </p>
     
     <li>
     <%= link_to '閲覧', "/tweets/#{tweet.id}", method: :get %>
     
     <% if user_signed_in? && current_user.id == tweet.user.id %>
      
     <%= link_to '編集', "/tweets/#{tweet.id}/edit", method: :get %>
     <%= link_to '削除', "/tweets/#{tweet.id}", method: :delete %>
     </li>
     
     
    <% end %>

    <% end %>
    <%= paginate(@tweets) %>
</div>